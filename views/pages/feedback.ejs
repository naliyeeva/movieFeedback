<%- include('header'); -%>

    <div class="container">
        <div class="row">
            <div class="parent-movies" id="get-feedback">
                <div class="rate mb-3">
                        <span class="star" id="begin"></span>
                        <button value="1" type="button" class="numbers-button"><span class="number">1</span></button>
                        <button value="2" type="button" class="numbers-button"><span class="number">2</span></button>
                        <button value="3" type="button" class="numbers-button"><span class="number">3</span></button>
                        <button value="4" type="button" class="numbers-button"><span class="number">4</span></button>
                        <button value="5" type="button" class="numbers-button"><span class="number">5</span></button>
                        <button value="6" type="button" class="numbers-button"><span class="number">6</span></button>
                        <button value="7" type="button" class="numbers-button"><span class="number">7</span></button>
                        <button value="8" type="button" class="numbers-button"><span class="number">8</span></button>
                        <button value="9" type="button" class="numbers-button"><span class="number">9</span></button>
                        <button value="10" type="button" class="numbers-button active"><span class="number">10</span></button>
                        <span class="star" id="end"></span>
                    </div>
                
                    <div class="mb-3">
                        <label for="exampleFormControlInput1" class="form-label">Email Address</label>
                        <input type="email" class="form-control" id="email" placeholder="name@example.com" required>
                    </div>
                    <div>
                        <label for="exampleFormControlTextarea1" class="form-label">Give Feedback</label>
                        <textarea class="form-control" id="feedback_text" rows="3" required></textarea>
                    </div>
                    <div class="actions my-4">
                        <div class="d-flex">
                            <a class="btn btn-light" href="movies">Go back to the list</a>
                        </div>
                        <div class="d-flex">
                            <a class="btn btn-light" id="submit-feedback">Submit</a>
                        </div>
                    </div>
            </div>
        </div>
    </div>


<%- include('footer'); -%>